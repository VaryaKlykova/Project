{
    "compilerOptions": {
      
      "target": "ES5",                         
      "module": "commonjs",                    
      
      "sourceMap": true,                     
       "removeComments": true,               
      
      "strict": true,                            
     
      "noUnusedLocals": true,               
      "noUnusedParameters": true,            
      "noImplicitReturns": true,             
      "noFallthroughCasesInSwitch": true,    
   
    }
